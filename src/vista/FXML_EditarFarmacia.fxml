<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="370.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="controlador.FXML_EditarFarmaciaController">
   <children>
      <GridPane layoutX="85.0" layoutY="110.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="BOTTOM_LEFT" text="ID" />
            <Label alignment="BOTTOM_LEFT" text="Nombre" GridPane.rowIndex="1" />
            <Label alignment="BOTTOM_LEFT" text="Password" GridPane.rowIndex="2" />
            <Label alignment="BOTTOM_LEFT" text="Dirección" GridPane.rowIndex="3" />
            <Label alignment="BOTTOM_LEFT" text="C.P" GridPane.rowIndex="4" />
            <Label alignment="BOTTOM_LEFT" text="Teléfono" GridPane.rowIndex="5" />
