         <buttons>
            <Button fx:id="btn_Aceptar" mnemonicParsing="false" onAction="#aceptar" style="-fx-background-color: #6f95ed;" text="Aceptar">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="btn_Cancelar" mnemonicParsing="false" onAction="#cancelar" style="-fx-background-color: #ea8984;" text="Cancelar">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </buttons>
         <padding>
            <Insets right="60.0" />
         </padding>
      </ButtonBar>
