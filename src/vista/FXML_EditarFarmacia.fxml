            <JFXTextField fx:id="txt_Nombre" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <JFXTextField fx:id="txt_Password" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <JFXTextField fx:id="txt_Direccion" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <JFXTextField fx:id="txt_CP" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <JFXTextField fx:id="txt_Telefono" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         </children>
      </GridPane>
      <FontAwesomeIconView fill="#6f95ed" glyphName="HEART" layoutX="160.0" layoutY="57.0" size="50" />
